  "description": "LinkedIn profile scraper using Puppeteer headless browser. So you can use it on a server. Returns structured LinkedIn public profile data in JSON format.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "ts-node-dev -r dotenv/config --inspect=9230 --respawn --transpileOnly ./src/index.ts",
    "build": "npx tsc -p tsconfig.production.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jordy van den Aardweg <jordyvandenaardweg@gmail.com> (https://github.com/jvandenaardweg)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jvandenaardweg/linkedin-profile-scraper.git"
  },
  "license": "ISC",
  "dependencies": {
    "clean-text-utils": "^1.1.17",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "puppeteer": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/puppeteer": "^2.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslib": "^1.11.2",
