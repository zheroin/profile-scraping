{
  "name": "linkedin-profile-scraper",
  "version": "1.0.0",
  "description": "LinkedIn profile scraper using Puppeteer headless browser. So you can use it on a server. Returns structured LinkedIn public profile data in JSON format.",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev -r dotenv/config --inspect=9230 --respawn --transpileOnly ./src/index.ts",
    "build": "npx tsc -p tsconfig.production.json",
    "test:watch": "NODE_ENV=test jest --watch --coverage",
    "test": "NODE_ENV=test jest --silent --maxWorkers=4"
  },
  "author": "Jordy van den Aardweg <jordyvandenaardweg@gmail.com> (https://github.com/jvandenaardweg)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jvandenaardweg/linkedin-profile-scraper.git"
  },
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "puppeteer": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/puppeteer": "^2.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.5.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
